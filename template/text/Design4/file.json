  "content": "<!-- wp:amp/rowlayout {\"uniqueID\":\"_7aa\",\"columns\":1,\"colLayout\":\"equal\",\"topPadding\":24,\"bottomPadding\":15,\"blockAlignment\":\"wide\"} -->\n<div class=\"wp-block-amp-rowlayout cr alignwide\"><div id=\"ri_7aa\" class=\"ab  ri_7aa\"><div class=\"cw ab1 amp-gutter-default gvdefault rvtop rlequal tlinherit amp-m-colapse-left-to-right mrow\"><!-- wp:amp/column {\"uniqueID\":\"_8f2\"} -->\n<div class=\"wp-block-amp-column c c-1 cd_8f2\"><div class=\"ci\"><!-- wp:amp/rowlayout {\"uniqueID\":\"_aab\",\"colLayout\":\"equal\",\"topPadding\":26,\"bottomPadding\":15,\"firstColumnWidth\":35,\"secondColumnWidth\":65} -->\n<div class=\"wp-block-amp-rowlayout cr alignnone\"><div id=\"ri_aab\" class=\"ab  ri_aab\"><div class=\"cw ab2 amp-gutter-default gvdefault rvtop rlequal tlinherit amp-m-colapse-left-to-right mrow amp-custom-first-width-35 amp-custom-second-width-65\"><!-- wp:amp/column {\"uniqueID\":\"_b7e\"} -->\n<div class=\"wp-block-amp-column c c-1 cd_b7e\"><div class=\"ci\"><!-- wp:ampblocks/image {\"width\":380,\"height\":550,\"imageurl\":\"https://raw.githubusercontent.com/jasthilokesh/sampletest/master/images/td-6.png\"} -->\n<div class=\"wp-block-ampblocks-image imw\"><div class=\"imc\"><img class=\"im-t\" width=\"380\" height=\"550\" src=\"https://raw.githubusercontent.com/jasthilokesh/sampletest/master/images/td-6.png\"/></div></div>\n<!-- /wp:ampblocks/image --></div></div>\n<!-- /wp:amp/column -->\n\n<!-- wp:amp/column {\"id\":2,\"leftPadding\":100,\"leftPaddingM\":0,\"uniqueID\":\"_2da\",\"leftPaddingT\":30} -->\n<div class=\"wp-block-amp-column c c-2 cd_2da\"><div class=\"ci\"><!-- wp:ampblocks/heading {\"uniqueID\":\"_886\",\"size\":48,\"lineHeight\":62,\"tabSize\":40,\"tabLineHeight\":52,\"mobileSize\":36,\"mobileLineHeight\":44,\"typography\":\"Montserrat\",\"googleFont\":true,\"fontSubset\":\"latin\",\"fontVariant\":\"900\",\"fontWeight\":\"900\"} -->\n<h2 id=\"h_886\" class=\"h_886 hp wp-block-amp-advancedheading\" style=\"margi